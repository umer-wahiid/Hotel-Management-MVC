    <script>
        $(document).ready(function () {
            //$("#abcd").click(function () {

            $.ajax({
                url: "/Contacts/GetContacts", // Replace with the appropriate controller and action URL
                type: "GET",
                dataType: "json",
                success: function (data) {
                    console.log(data)
                    // Iterate through the retrieved contacts and populate the HTML dynamically
                    var html = "";
                    data.forEach(function (contact) {
                        html += '<a href="#" class="media-list-link read">';
                        html += '<div class="media pd-x-20 pd-y-15">';
                        html += '<img src="https://api.dicebear.com/6.x/pixel-art/svg?seed='+contact.UName+'" class="wd-40 rounded-circle" alt="">';
                        html += '<div class="media-body">';
                        html += '<p class="tx-13 mg-b-0" style="text-overflow: ellipsis;white-space: nowrap;display: block;overflow: hidden;width: 17em;"><strong class="tx-medium">' + contact.UName + '</strong>'+'    ' + contact.Message + '</p>';
                        html += '<span class="tx-12">' + contact.Email + '</span>';
                        html += '</div></div><!-- media --></a>';
                    });
                    // Append the generated HTML to the contacts link container
                    $('#contactsContainer').html(html);
                    if (html.length == 0) {
                        var abc = '<span id="dot" hidden class="square-8 bg-danger pos-absolute t-15 r-0 rounded-circle"></span>';
                        $('#dott').html(abc);
                    }
                },
                error: function (xhr, status, error) {
                    console.log(error);
                }
            //});
            });
        });





        $("#read").click(function () {
            $.post({
                url: '@Url.Action("Read", "Contacts")',
                //async:true,
              success: function(response) {
                  console.log("Form submission Success: " + response.message);
                  //window.location.href = '@Url.Action("Index", "Admins")';

              },
              error: function(xhr, status, error) {
                  console.log("An error occurred during form submission: " + error);
              }
            });
        });
    </script>
